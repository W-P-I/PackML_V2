							<Line Number="9">
								<![CDATA[//           derivative]]>
							</Line>
							<Line Number="10">
								<![CDATA[//           ]]>
							</Line>
							<Line Number="11">
								<![CDATA[//]]>
							</Line>
							<Line Number="12">
								<![CDATA[//   History:]]>
							</Line>
							<Line Number="13">
								<![CDATA[//]]>
							</Line>
							<Line Number="14">
								<![CDATA[////////////////////////////////////////////////////////////]]>
							</Line>
							<Line Number="15">
								<![CDATA[#region Timer (dt)]]>
							</Line>
							<Line Number="16">
								<![CDATA[	time_interval.TimerEnable:=Enable_PID;]]>
							</Line>
							<Line Number="17">
								<![CDATA[	TONR(time_interval);]]>
							</Line>
							<Line Number="18">
								<![CDATA[	time_interval.Reset:=0;]]>
							</Line>
							<Line Number="19">
								<![CDATA[	time_interval.PRE:=dt*1000;]]>
							</Line>
							<Line Number="20">
								<![CDATA[#endregion]]>
							</Line>
							<Line Number="21">
								<![CDATA[#region Startup PID]]>
							</Line>
							<Line Number="22">
								<![CDATA[if Kp <> wrk_kp then cnf_setPID_Values:=1; wrk_kp:=Kp; end_if;]]>
							</Line>
							<Line Number="23">
								<![CDATA[if Ki <> wrk_ki then cnf_setPID_Values:=1; wrk_ki:=Ki; end_if;]]>
							</Line>
							<Line Number="24">
								<![CDATA[if Kd <> wrk_kd then cnf_setPID_Values:=1; wrk_kd:=Kd; end_if;]]>
							</Line>
							<Line Number="25">
								<![CDATA[if dt <> wrk_dt then cnf_setPID_Values:=1; wrk_dt:=dt; end_if;]]>
							</Line>
							<Line Number="26">
								<![CDATA[if N  <> wrk_N  then cnf_setPID_Values:=1; wrk_N :=N;  end_if;]]>
							</Line>
							<Line Number="27">
								<![CDATA[]]>
							</Line>
							<Line Number="28">
								<![CDATA[	if cnf_setPID_Values AND Enable_PID then]]>
							</Line>
							<Line Number="29">
								<![CDATA[		A0 := Kp + Ki*dt;]]>
							</Line>
							<Line Number="30">
								<![CDATA[		A1 := 0-Kp;]]>
							</Line>
							<Line Number="31">
								<![CDATA[		error[2] := 0; // e(t-2)]]>
							</Line>
							<Line Number="32">
								<![CDATA[		error[1] := 0; // e(t-1)]]>
							</Line>
							<Line Number="33">
								<![CDATA[		error[0] := 0; // e(t)]]>
							</Line>
							<Line Number="34">
								<![CDATA[		Output := actuator_value;  // Usually the current value of the actuator]]>
							</Line>
							<Line Number="35">
								<![CDATA[		A0d:= Kd/dt;]]>
							</Line>
							<Line Number="36">
								<![CDATA[		A1d:= (0- 2.0)*Kd/dt;]]>
							</Line>
							<Line Number="37">
								<![CDATA[		A2d:= Kd/dt;]]>
							</Line>
							<Line Number="38">
								<![CDATA[		N := N;]]>
							</Line>
							<Line Number="39">
								<![CDATA[		tau := Kd / (Kp*N); // IIR filter time constant]]>
							</Line>
							<Line Number="40">
								<![CDATA[		alpha:= dt / (2*tau);]]>
							</Line>
							<Line Number="41">
								<![CDATA[		d0 := 0;]]>
							</Line>
							<Line Number="42">
								<![CDATA[		d1 := 0;]]>
							</Line>
							<Line Number="43">
								<![CDATA[		fd0 := 0;]]>
							</Line>
							<Line Number="44">
								<![CDATA[		fd1 := 0;]]>
							</Line>
							<Line Number="45">
								<![CDATA[		if NOT cnf_Use_PID.0 then cnf_Use_PID.0 := 1; end_if;// makesure P is always active.]]>
							</Line>
							<Line Number="46">
								<![CDATA[		cnf_setPID_Values:=0;]]>
							</Line>
							<Line Number="47">
								<![CDATA[	end_if;]]>
							</Line>
							<Line Number="48">
								<![CDATA[	]]>
							</Line>
							<Line Number="49">
								<![CDATA[#endregion]]>
							</Line>
							<Line Number="50">
								<![CDATA[#region PID logic, constant loop]]>
							</Line>
							<Line Number="51">
								<![CDATA[	if time_interval.DN then]]>
							</Line>
							<Line Number="52">
								<![CDATA[	    error[2] := error[1];]]>
							</Line>
							<Line Number="53">
								<![CDATA[	    error[1] := error[0];]]>
							</Line>
							<Line Number="54">
								<![CDATA[	    error[0] := setpoint - measured_value;]]>
							</Line>
							<Line Number="55">
								<![CDATA[		wrk_output:=Output;]]>
							</Line>
							<Line Number="56">
								<![CDATA[]]>
							</Line>
							<Line Number="57">
								<![CDATA[		case cnf_Use_PID of]]>
							</Line>
							<Line Number="58">
								<![CDATA[			0: cnf_Use_PID:=1;]]>
							</Line>
							<Line Number="59">
								<![CDATA[			1: // P 	controller]]>
							</Line>
							<Line Number="60">
								<![CDATA[				proportional := error[0];]]>
							</Line>
							<Line Number="61">
								<![CDATA[				wrk_output := Kp * proportional;]]>
							</Line>
							<Line Number="62">
								<![CDATA[			3: // PI 	controller]]>
							</Line>
							<Line Number="63">
								<![CDATA[				wrk_output := wrk_output + A0 * error[0] + A1 * error[1];]]>
							</Line>
							<Line Number="64">
								<![CDATA[			5: // PD 	controller]]>
							</Line>
							<Line Number="65">
								<![CDATA[				proportional := error[0];]]>
							</Line>
							<Line Number="66">
								<![CDATA[				wrk_output := Kp * proportional;]]>
							</Line>
							<Line Number="67">
								<![CDATA[				d1 := d0;]]>
							</Line>
							<Line Number="68">
								<![CDATA[			    d0 := A0d * error[0] + A1d * error[1] + A2d * error[2];]]>
							</Line>
							<Line Number="69">
								<![CDATA[			    fd1 := fd0;]]>
							</Line>
							<Line Number="70">
								<![CDATA[			    fd0 := ((alpha) / (alpha + 1)) * (d0 + d1) - ((alpha - 1) / (alpha + 1)) * fd1;]]>
							</Line>
							<Line Number="71">
								<![CDATA[			    wrk_output := wrk_output + fd0;]]>
							</Line>
							<Line Number="72">
								<![CDATA[			7: // PID 	controller]]>
							</Line>
							<Line Number="73">
								<![CDATA[				wrk_output := wrk_output + A0 * error[0] + A1 * error[1];]]>
							</Line>
							<Line Number="74">
								<![CDATA[			    d1 := d0;]]>
							</Line>
							<Line Number="75">
								<![CDATA[			    d0 := A0d * error[0] + A1d * error[1] + A2d * error[2];]]>
							</Line>
							<Line Number="76">
								<![CDATA[			    fd1 := fd0;]]>
							</Line>
							<Line Number="77">
								<![CDATA[			    fd0 := ((alpha) / (alpha + 1)) * (d0 + d1) - ((alpha - 1) / (alpha + 1)) * fd1;]]>
							</Line>
							<Line Number="78">
								<![CDATA[			    wrk_output := wrk_output + fd0;]]>
							</Line>
							<Line Number="79">
								<![CDATA[			else]]>
							</Line>
							<Line Number="80">
								<![CDATA[				cnf_Use_PID:=1;]]>
							</Line>
							<Line Number="81">
								<![CDATA[		end_case;]]>
							</Line>
							<Line Number="82">
								<![CDATA[		if wrk_output>=100 then wrk_output:=100; end_if;]]>
							</Line>
							<Line Number="83">
								<![CDATA[		if 0>=wrk_output then wrk_output:=0; end_if;]]>
							</Line>
							<Line Number="84">
								<![CDATA[		Output:=wrk_output;]]>
							</Line>
							<Line Number="85">
								<![CDATA[		time_interval.Reset:=1;]]>
							</Line>
							<Line Number="86">
								<![CDATA[	end_if;]]>
							</Line>
							<Line Number="87">
								<![CDATA[#endregion]]>
							</Line>
							<Line Number="88">
								<![CDATA[#region PID Handvalue]]>
							</Line>
							<Line Number="89">
								<![CDATA[	if NOT Enable_PID then]]>
							</Line>
							<Line Number="90">
								<![CDATA[		Output:=HandValue;]]>
							</Line>
							<Line Number="91">
								<![CDATA[	end_if;]]>
							</Line>
							<Line Number="92">
								<![CDATA[#endregion]]>
							</Line>
						</STContent>
					</Routine>
