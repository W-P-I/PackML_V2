</Line>
<Line Number="13">
<![CDATA[if 		Unit.Sts_Production 	then	wrk_mode_Select:=1;]]>
</Line>
<Line Number="14">
<![CDATA[elsif 	Unit.Sts_Maintenance 	then	wrk_mode_Select:=2;]]>
</Line>
<Line Number="15">
<![CDATA[elsif 	Unit.Sts_Manual 		then	wrk_mode_Select:=3;]]>
</Line>
<Line Number="16">
<![CDATA[elsif 	Unit.Sts_Cleaning 		then	wrk_mode_Select:=4;]]>
</Line>
<Line Number="17">
<![CDATA[elsif 	Unit.Sts_MES 			then	wrk_mode_Select:=5;]]>
</Line>
<Line Number="18">
<![CDATA[elsif 	Unit.Sts_SemiAutomatic 	then	wrk_mode_Select:=6;				]]>
</Line>
<Line Number="19">
<![CDATA[end_if;]]>
</Line>
<Line Number="20">
<![CDATA[#region Set case state non active states]]>
</Line>
<Line Number="21">
<![CDATA[	if 		Unit.Sts_Aborted 		then	wrk_state_Select:=9;]]>
</Line>
<Line Number="22">
<![CDATA[	elsif 	Unit.Sts_Stopped		then	wrk_state_Select:=2;]]>
</Line>
<Line Number="23">
<![CDATA[	elsif   Unit.Sts_Idle			then	wrk_state_Select:=4;]]>
</Line>
<Line Number="24">
<![CDATA[	elsif 	Unit.Sts_Execute		then	wrk_state_Select:=6;]]>
</Line>
<Line Number="25">
<![CDATA[	elsif 	Unit.Sts_Held			then	wrk_state_Select:=11;]]>
</Line>
<Line Number="26">
<![CDATA[	elsif 	Unit.Sts_Suspended		then	wrk_state_Select:=5;]]>
</Line>
<Line Number="27">
<![CDATA[	elsif   Unit.Sts_Complete		then	wrk_state_Select:=17;			]]>
</Line>
<Line Number="28">
<![CDATA[	end_if;]]>
</Line>
<Line Number="29">
<![CDATA[#endregion]]>
</Line>
<Line Number="30">
<![CDATA[#region Set case state]]>
</Line>
<Line Number="31">
<![CDATA[	if 		Unit.Sts_Production AND Unit.Sts_Aborting 		then	wrk_state_Select:=8;]]>
</Line>
<Line Number="32">
<![CDATA[	elsif 	Unit.Sts_Production AND Unit.Sts_Clearing		then	wrk_state_Select:=1;]]>
